{
  "name": "calories-record",
  "version": "1.0.0",
  "author": "YukiEndo",
  "license": "MIT",
  "private": true,
  "scripts": {
    "stylelint": "stylelint \"./src/scripts/**/*.tsx\"",
    "eslint": "eslint --fix \"./src/scripts/**/*.ts\"",
    "dev": "NODE_ENV=development webpack-dev-server --config ./webpack.config.js",
    "build": "yarn eslint && yarn stylelint && NODE_ENV=production webpack --colors --config ./webpack.config.js",
    "api-server": "node api/index.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/scripts/**/*.ts": [
      "yarn eslint"
    ],
    "src/scripts/**/*.tsx": [
      "yarn stylelint"
    ]
  },
  "dependencies": {
    "core-js": "^3.8.0",
    "express": "^4.17.1",
    "mongoose": "^5.12.14",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@reduxjs/toolkit": "^1.5.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.11",
    "@types/react-router-dom": "^5.1.6",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "redux-devtools-extension": "^2.13.8",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "terser-webpack-plugin": "4.1.0",
    "typescript": "^4.1.2",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  }
}
